curl --header "Content-Type: application/json" --request POST --data '{"name":"Ayniee","age":56, "occupation":"Thief"}' http://127.0.0.1:5000/user

curl --header "Content-Type: application/json" --request DELETE http://127.0.0.1:5000/user/Jas

curl --request POST \
  --url http://127.0.0.1:5000/user/ \
  --header 'age: 22' \
  --header 'name: Ayniee' \
  --header 'occupation: Engineer'
